<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Multibrowsertesting" parallel="tests" thread-count="5">
  <test name="Chrome Test">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="testscripts.ManageCategoryTest"/>
      <class name="testscripts.ManageNewsTest"/>
      <class name="testscripts.LogOutTest"/>
      <class name="testscripts.ManageContactTest"/>
      <class name="testscripts.FooterTest"/>
      <class name="testscripts.AdminUserTest"/>
      <class name="testscripts.LoginTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="Edge Test">
  <parameter name="browser" value="edge"></parameter>
    <classes>
     <class name="testscripts.ManageCategoryTest"/>
      <class name="testscripts.ManageNewsTest"/>
      <class name="testscripts.LogOutTest"/>
      <class name="testscripts.ManageContactTest"/>
      <class name="testscripts.FooterTest"/>
      <class name="testscripts.AdminUserTest"/>
      <class name="testscripts.LoginTest"/>
    </classes>
  </test>
</suite> <!-- Suite -->
